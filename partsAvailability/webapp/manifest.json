{
	"_version": "1.5.0",
	"sap.app": {
		"id": "partsAvailability",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {

		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "partsAvailability.view.searchMaterialandDisplay",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "partsAvailability.i18n.i18n"
				}
			}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
	"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "zdi.view",
				"controlId": "idAppControl",
				"clearTarget": false,
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"PartssearchandDisplay"
					]
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "PartssearchandDisplay",
					"target": [
						"PartssearchandDisplay"
					]
				} 
				
			],
			"targets": {
				"PartssearchandDisplay": {
					"viewName": "searchMaterialandDisplay",
					"viewLevel": 1,
					"transition": "slide"
				} 
			}
		}	
	}
}