{
	"_version": "1.5.0",
	"sap.app": {
		"id": "partsAvailability",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"MD_PRODUCT_OP_SRV": {
				"uri": "/node/MD_PRODUCT_OP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/MD_PRODUCT_OP_SRV/metadata.xml"
				}
			},
			"ZMD_PRODUCT_FS_V2_SRV": {
				"uri": "/node/ZMD_PRODUCT_FS_V2_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZMD_PRODUCT_FS_V2_SRV/metadata.xml"
				}
			},
			"API_BUSINESS_PARTNER": {
				"uri": "/node/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/API_BUSINESS_PARTNER/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "partsAvailability.view.searchMaterialandDisplay",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "partsAvailability.i18n.i18n"
				}
			},
			"materialDisplayModeloData": {
				"uri": "/node/MD_PRODUCT_OP_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "MD_PRODUCT_OP_SRV",
				"preload": true
			},
			"zMaterialDisplayModel": {
				"uri": "/node/ZMD_PRODUCT_FS_V2_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "ZMD_PRODUCT_FS_V2_SRV",
				"preload": true
			},
			"aPiBusinessPartner": {
				"uri": "/node/API_BUSINESS_PARTNER/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "API_BUSINESS_PARTNER",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "zdi.view",
				"controlId": "idAppControl",
				"clearTarget": false,
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"PartssearchandDisplay"
					]
				}
			},
			"routes": [{
					"pattern": "",
					"name": "PartssearchandDisplay",
					"target": [
						"PartssearchandDisplay"
					]
				}

			],
			"targets": {
				"PartssearchandDisplay": {
					"viewName": "searchMaterialandDisplay",
					"viewLevel": 1,
					"transition": "slide"
				}
			}
		}
	}
}